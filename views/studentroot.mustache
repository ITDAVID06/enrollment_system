<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Root</title>
    <link rel="stylesheet" href="views/assets/css/sidebar.css">
    <link rel="stylesheet" href="views/assets/css/navbar.css">
    <link rel="stylesheet" href="views/assets/css/dashboard.css">
</head>
<body>
    <div id="app-container" class="app-container">
        {{> studentsidebar}}

        <div id="main-content" class="main-content">
            <div id="navbar1" class="navbar1">{{> navigation}}</div>
            <div id="panel" class="panel">
                    {{#isProfile}}
                    {{> studentprofile}}
                    {{/isProfile}}

                    {{#isViewSheet}}
                    {{> studentview}}
                    {{/isViewSheet}}

                    {{#isEditSheet}}
                    {{> studentedit}}
                    {{/isEditSheet}}

                    {{#isRegistration}}
                    {{> studentregistration}}
                    {{/isRegistration}}
                    {{#isSchedule}}
                    {{> studentschedule}}
                    {{/isSchedule}}
                    {{#isAccount}}
                    {{> studentaccount}}
                    {{/isAccount}}
            </div>
        </div> 
    </div>
    <script src="views/assets/js/sidebar.js"></script>
</body>
</html>
